@{
    Layout = null;
}

@(

    @Html.HighchartSetUp("container")
         .Title("Chart Title")
         .Subtitle("Now: " + DateTime.Now.ToString())
         .AxisX("Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec")
         .AxisY("Quantity")
         .Series(
            AjaxConfig.LoadFrom(Url.Action("LoadData", "Ajax"))
        )
         .ToHtmlString()
)